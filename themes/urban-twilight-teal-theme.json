  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#c084fc"
    },
    "variable.constant": {
      "foreground": "#fde68a"
    },
    "variable.defaultLibrary": {
      "foreground": "#fde6a0"
    }
  },
  "tokenColors": [
    {
      "name": "accent",
      "scope": ["accent.xi"],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "Attribute IDs",
      "scope": ["entity.other.attribute-name.id", "entity.other.attribute-name.class.css"],
      "settings": {
        "fontStyle": "",
        "foreground": "#60a5fa"
      }
